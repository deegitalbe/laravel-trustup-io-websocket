{
    "name": "deegitalbe/laravel-trustup-io-websocket",
    "description": "Used to interact with our websocket microservice",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mathieu Henrotay",
            "email": "mathieu.henrotay@gmail.com"
        }
    ],
    "require": {
        "henrotaym/laravel-package-versioning-config": "^2.0",
        "pusher/pusher-php-server": "^7.2"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Deegitalbe\\LaravelTrustupIoWebsocket\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Deegitalbe\\LaravelTrustupIoWebsocket\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Deegitalbe\\LaravelTrustupIoWebsocket\\Providers\\LaravelTrustupIoWebsocketServiceProvider"
            ]
        }
    }
}
